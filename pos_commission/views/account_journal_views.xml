<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_journal_pos_commission_form" model="ir.ui.view">
        <field name="name">account.journal.pos.commission.form.inherit</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type']" position="after">
                <field string="Is Commission" name="is_commission"/>
            </xpath>
        </field>
    </record>
</odoo>